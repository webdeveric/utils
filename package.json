{
  "name": "@webdeveric/utils",
  "description": "Just another collection of utilities.",
  "version": "0.14.0",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "utilities",
    "asArray",
    "clamp",
    "compose",
    "convert",
    "delay",
    "delayAnimationFrame",
    "fcrdns",
    "getMaxValue",
    "getMinValue",
    "getOwnKeys",
    "getOwnProperties",
    "getType",
    "isEmpty",
    "isObject",
    "joinStrings",
    "normalize",
    "secToString",
    "toPascalCase",
    "trimEnd",
    "trimStart",
    "type-assertion",
    "type-predicate",
    "uniqueItems",
    "until"
  ],
  "author": {
    "email": "eric@webdeveric.com",
    "name": "Eric King",
    "url": "http://webdeveric.com/"
  },
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "engines": {
    "node": ">=14.18.0"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./*": {
      "require": "./dist/cjs/*",
      "import": "./dist/mjs/*"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/webdeveric/utils.git"
  },
  "bugs": {
    "url": "https://github.com/webdeveric/utils/issues"
  },
  "homepage": "https://github.com/webdeveric/utils/#readme",
  "scripts": {
    "clean": "rimraf ./dist/",
    "prebuild": "npm run clean",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:mjs": "tsc -p ./tsconfig.mjs.json",
    "postbuild:cjs": "echo '{\"type\":\"commonjs\"}' > ./dist/cjs/package.json",
    "postbuild:mjs": "echo '{\"type\":\"module\"}' > ./dist/mjs/package.json",
    "build": "npm run build:cjs && npm run build:mjs",
    "postbuild": "npm run validate",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./*.js ./bin/ ./src/ ./test/ --ext .ts,.mjs,.cjs",
    "test": "NODE_ENV=test jest",
    "validate": "./bin/validate.cjs",
    "update-keywords": "./bin/update-keywords.mjs",
    "spellcheck": "cspell './{.github,src,test}/**/*.{ts,json}' './*.{md,js}' './package.json' --no-progress",
    "prepublishOnly": "npm run typecheck && npm run spellcheck && npm run lint && npm test && npm run build",
    "format": "prettier --write ./*.{js,json,md} ./bin/ ./src/ ./test/",
    "prepare": "husky install"
  },
  "prettier": "@webdeveric/prettier-config",
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{ts,js,json,md}": [
      "cspell lint --no-progress --no-summary --no-must-find-files",
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.escaperegexp": "^4.1.2",
    "ts-essentials": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.escaperegexp": "^4.1.6",
    "@types/node": "^17.0.10",
    "@webdeveric/eslint-config-ts": "^0.5.2",
    "@webdeveric/prettier-config": "^0.2.0",
    "core-js": "^3.11.1",
    "cspell": "^6.1.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.7.3"
  }
}
