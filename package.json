{
  "name": "@webdeveric/utils",
  "description": "Just another collection of utilities.",
  "version": "0.26.2",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "utilities",
    "asArray",
    "capitalize",
    "clamp",
    "compose",
    "convert",
    "delay",
    "delayAnimationFrame",
    "describeInput",
    "getMaxValue",
    "getMinValue",
    "getOwnKeys",
    "getOwnProperties",
    "getType",
    "isEmpty",
    "isObject",
    "joinStrings",
    "looksLikeURL",
    "normalize",
    "redactCredentialsInURL",
    "secToString",
    "toPascalCase",
    "trimEnd",
    "trimIndentation",
    "trimStart",
    "type-assertion",
    "type-predicate",
    "uniqueItems",
    "until"
  ],
  "author": {
    "email": "eric@webdeveric.com",
    "name": "Eric King",
    "url": "http://webdeveric.com/"
  },
  "type": "commonjs",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "engines": {
    "node": ">=14.19.0"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/mjs/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/webdeveric/utils.git"
  },
  "bugs": {
    "url": "https://github.com/webdeveric/utils/issues"
  },
  "homepage": "https://github.com/webdeveric/utils/#readme",
  "packageManager": "npm@9.2.0",
  "scripts": {
    "clean": "rimraf ./dist/",
    "prebuild": "npm run clean",
    "build:cjs": "tsc -p ./tsconfig.build-cjs.json",
    "build:mjs": "tsc -p ./tsconfig.build-mjs.json",
    "postbuild:cjs": "echo '{\"type\":\"commonjs\"}' > ./dist/cjs/package.json",
    "postbuild:mjs": "echo '{\"type\":\"module\"}' > ./dist/mjs/package.json",
    "build": "npm run build:cjs && npm run build:mjs",
    "postbuild": "npm run validate",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./*.{js,cjs,mjs,ts,cts,mts} ./bin/ ./src/ ./test/ --ext .ts,.mjs,.cjs",
    "test": "vitest",
    "coverage": "vitest run  -c vitest.config.mts --coverage",
    "validate": "./bin/validate.cjs",
    "update-keywords": "./bin/update-keywords.mjs",
    "spellcheck": "cspell './{.github,src,test}/**/*.{ts,json}' './*.{md,js}' './package.json' --no-progress",
    "prepublishOnly": "npm run typecheck && npm run spellcheck && npm run lint && npm run coverage && npm run build",
    "format": "prettier --write ./*.{js,json,md} ./bin/ ./src/ ./test/",
    "prepare": "husky install"
  },
  "prettier": "@webdeveric/prettier-config",
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.escaperegexp": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.escaperegexp": "^4.1.7",
    "@types/node": "^18.11.12",
    "@vitest/coverage-c8": "^0.29.1",
    "@webdeveric/eslint-config-ts": "^0.6.0",
    "@webdeveric/prettier-config": "^0.2.0",
    "cspell": "^6.19.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.1",
    "typescript": "^5.0.2",
    "vitest": "^0.29.1"
  }
}
